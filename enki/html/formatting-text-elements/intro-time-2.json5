  card: {
    question: '<h3>How much do you know about the <code>&#x3C;time></code> element?</h3>\n<p>Match the appropriate datetime attributes:</p>\n<p>Dec 25th, 1975 at 6pm: <code>&#x3C;time datetime="???"></code>\nThree-Day Event: <code>&#x3C;time datetime="???"></code></p>\n',
    answers: [
      {
