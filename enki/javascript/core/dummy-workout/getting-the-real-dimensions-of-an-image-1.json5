  card: {
    question: "<p>Complete the load listener of <code>enkiImg</code> to get natural width and height:</p>\n<pre><code class=\"language-javasscript\">enkiImg.???('???',\n function() {\n    console.log('Width: ',\n this.???);\n    console.log('Height: ',\n this.???);\n});\n</code></pre>\n",
